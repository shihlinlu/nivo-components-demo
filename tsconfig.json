{
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "sourceMap": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "main-styled-components": ["themes/styledComponents.ts"],
      "pages/*": ["pages/*"],
      "resources/*": ["resources/*"],
      "themes/*": ["themes/*"],
      "utility/*": ["utility/*"],
      "src/*": ["*"],
      "data/*": ["data/*"],
      "charts/*": ["charts/*"]
    },
    "lib": ["es2017", "es2018", "dom"],
    "skipLibCheck": true
  },
  "include": ["./src/**/*", "types/*"]
}
